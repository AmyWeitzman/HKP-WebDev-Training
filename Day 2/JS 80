var count = 0;

function cc(card) {
  // Only change code below this line
  if(card <= 6) {
    count++;
  }
  else if(card != 7 && card != 8 && card != 9) {
    count--;
  } 

  var res = (count > 0) ? "Bet" : "Hold";
  return count + " " + res;
  // Only change code above this line
}

cc(2); cc(3); cc(7); cc('K'); cc('A');
